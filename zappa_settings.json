{
    "production": {
        "app_function": "src.twenty_four.app.http.app",
        "aws_region": "us-west-2",
        "project_name": "service-template",
        "runtime": "python3.8",
        "s3_bucket": "zappa-w8a1r46rr",
        "log_level": "INFO",
        "timeout_seconds": 55,
        "events": [{
                "function": "src.service_template.app.handlers.event_driven_task",
                "event_source": {
                    "arn": "arn:aws:sqs:us-west-2:120356305272:PROD-service-template-event-driven",
                    "batch_size": 1,
                    "enabled": false
                }
            },
            {
                "function": "src.service_template.app.handlers.time_driven_task",
                "expression": "cron(18 3 ? * * *)",
                "enabled": false
            }
        ],
        "aws_environment_variables": {
            "STAGE": "PROD"
        }
    }
}